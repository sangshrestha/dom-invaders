{
  "manifest_version": 3,
  "name": "Dom Invaders",
  "version": "1.0",
  "background": {
    "service_worker": "background.js"
  },
  "action": {},
  "permissions": ["activeTab", "scripting"],
  "web_accessible_resources": [
    {
      "resources": ["assets/*"],
      "matches": ["<all_urls>"]
    }
  ]
}
